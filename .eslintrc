{
  "root": true,
  "env": {
      "es6": true,
      "node": true,
      "mocha": true
  },
  "extends": "airbnb-base",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
    }
  },
  "plugins": [
      "react", "import"
  ],
  "rules": {
    "linebreak-style": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0
  }
}